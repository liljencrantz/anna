/*
  Test template specialization of function names in types
*/
type Base
{}

type Foo (template(%op:__add__), extends(Base), pie) 
{

    def Int bar(Int a, Int b)
    {
        a.%op(b);
    }

}

def Int main()
{
    foo := Foo();
    print(foo.bar(3,4),"\n");
    boo := Foo«__sub__»();
    print(boo.bar(3,4),"\n");
}
