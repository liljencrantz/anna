def task(mp.Channel serverChannel)
{
    val := serverChannel.read() as ImutableString;
    (0..10000).each(i)
    {
	unix.env["foo"] = val;
        curr := unix.env["foo"];
	if(!(curr in ["foo0", "foo1", "foo2", "foo3", "foo4", "foo5"]))
	{
	    print("Bad value", curr);
	}
    }
    print("Done");
}

def main()
{
    threads :== 100;//1 + (tm.usec ^mod 512);
    (0..threads).each(idx)
    {
	mp.run(task).write(("foo" ~ (idx ^mod 6)));
    }
}
