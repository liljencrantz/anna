    print(tz == timezoneName, "\n");

    print(breakTime(mkTime(0,1,2,3,4,100, ?, "America/Toronto"), "America/Toronto"), "\n");
    print(breakTime(mkTime(0,1,2,3,4,100, ?, ?), ?)[0..8], "\n");

}

