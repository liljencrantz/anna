/*
  Naive implementation of the
  Sieve of Eratosthenes
*/

def main()
{
    var end = 1000000;
    var sieve = [];
    var count = 0;
    (2..end).each(i)
    {
        if(!sieve[i])
        {
	    ((2*i)..end:i).each(j){
                sieve[j] = 1;
            }
            count++;
        }
    }
    print("There are ", count, " prime numbers under ", end, "\n");
}
