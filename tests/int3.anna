/*
  Test that over and underflows of addition and subtraction work
  correctly 
*/
def main()
{
    a:=0;
    b:=0;
    c:=0;
    d:=0;
    (0..100_000).each(i){a += 10_000}
    (0..100_000).each(i){b -= 10_000}
    (0..100_000).each(i){c += -10_000}
    (0..100_000).each(i){d -= -10_000}
    print(a, ' ', b, ' ', c, ' ', d, '\n');
}
