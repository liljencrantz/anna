/*
  Check that types like HashMap, that contain gaps between the
  members, can be extended correctly.
*/

type MidHash
(extends(HashMap«Int, Int»))
{
}

type MyHash
(extends(MidHash))
{
    var Int foo;
    var Int bar;
    var Int baz;
    var Int qux;
}

def main()
{
    hh := MyHash(3|7, 2|5, 0|99);
    print(hh, "\n");
    print(hh.foo, hh.bar, hh.baz, hh.qux, "\n");
    hh.foo = 1;
    hh.bar = 1;
    hh.baz = 1;
    hh.qux = 1;
    print(hh.foo, hh.bar, hh.baz, hh.qux, "\n");
}

