def foo(Int first, Int second, Int rest...){
    print(first, second, rest);
}

def main() (
	doc("Test that variadic arguments and named arguments mix properly")
    ){
    foo(first:1, second:2);
    foo(first:1, second:2, 3,4,5);
    foo(1, 2, 3, 4, 5);
    foo(1, 2);
    foo(3,4,5,first:1, second:2);
}
