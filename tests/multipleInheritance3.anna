/*
  Check that alias lookup works when resolving methods derived from
  different base classes.
 */

class Base1
{
    def intFoo(Int p) (alias(foo))
    {
	print("intFoo\n");
    }
}

class Base2
{
    def stringFoo(String p) (alias(foo))
    {
	print("stringFoo\n");
    }
}

class Derived1
(extends(Base1, Base2))
{
}

class Derived2
(extends(Base1, Base2))
{
}

def main()
{
    expandCode(
	%Type().foo(%val),
	Type: [Derived1, Derived1, Derived2, Derived2],
	val: ["", 1, "", 1]);
}

