def main()
{
    /*
      This file contains a few tests for the in operator of ranges
    */
    foo := 1..4;
    print(1 in foo);
    print(3 in foo);
    print(69 in foo);
    print(-12 in foo);

    print(7 in (3..9|2));
    print(9 in (3..9|2));
    print(9 in (3..?|2));
    print(999999 in (3..?|2));
    print(68 in (-1..?|69));
    print(-5 in (1..-20|-2));
    print(-5 in (1..?|-2));
    print(-4 in (1..?|-2));

}
