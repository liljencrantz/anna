
type MyType
{

    def staticMethod(Int idx, String in) (static)
    {
	print("Run method with arg ", in, "\n" );
	return "" ~in[0];
    }
    
    def yay()
    {
	print(["a","b", "c", "d", "e"].__map__(staticMethod));
    }
    
}

def main()
{
    mm := MyType();
    mm.yay();
    print("\n");
}

