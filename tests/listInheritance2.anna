/*
  Check that extending a specialized template class works
*/

type Derived
(
    extends(Int), extends(MutableList«Int»)
)
{
    def __init__()
    {
	Int::__init__(this, 7);
	MutableList«Int»::__init__(this, 0,10,1);
    }

}


def main()
{
    vv := Derived();
    vv[3] = 7;
    print(vv, '\n');
}
