class Derived1
(
    extends(Int, Range)
)
{
    def __init__()
    {
	Int::__init__(this, 0);
	Range::__init__(this, 0,10,1);
    }
}

class Derived2
(
    extends(Range, Int)
)
{
    def __init__()
    {
	Int::__init__(this, 0);
	Range::__init__(this, 0,10,1);
    }
}

def main()
{
    vv := Derived1();

    // Int.__add__
    print(vv + 5);

    // Range.__each__
    vv.each(i){print(i)}

    // Range.toString
    print(vv);

    vv2 := Derived2();

    // Int.__add__
    print(vv2 + 5);

    // Range.__each__
    vv2.each(i){print(i)}

    // Range.toString
    print(vv2);
}
