type Derived
(
    extends(Int), 
    extends(Range)
)
{
    def __init__()
    {
	Int.__init__(this, 0);
	Range.__init__(this, 0,10,1);
    }
}

def main()
{
    vv := Derived();

    // Int.__add__
    print(vv + 5, '\n');

    // Range.__each__
    vv.each(i){print(i, '\n')}

    // Range.toString
    print(vv, '\n');
}
