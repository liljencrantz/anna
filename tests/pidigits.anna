
def main()
{
    digits:=1000;

    i := 0;
    k := 0;
    ns := 0;
    k1 := 1;
    n := 1;
    a := 0;
    d := 1;
    t := 0;
    u:=0;

    def calc()
    {
	while(1)
	{
	    k += 1;
	    t = n ^shl 1;
	    n = n*k;
	    a += t;
	    k1 += 2;
	    a = a* k1;
	    d = d*k1;
	    if(a >= n){
		tmp := n*3+a;
		t = tmp/d;
		u = tmp ^mod d;
		u += n;
		if (d > u)
		{
		    ns = ns*10 + t;
		    i += 1;
		    if (i ^mod 10 == 0)
		    {
			print("%\t|%\n" % [ns, i]);
			ns = 0;
		    }
		    if (i >= digits){
			return;
		    }
		    a -= d*t;
		    a = a*10;
		    n = n*10;
		}
	    }
	}
    }
    calc();
}
