def main(){
    /* Perform a bunch of bitwise tests twise, once for large and once for small numbers */
    print(
	0b1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010 ^bitor 
        0b0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011 == 
	0b1011_1011_1011_1011_1011_1011_1011_1011_1011_1011_1011_1011);

    print(
	0b1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010 ^bitand
	0b0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011 == 
	0b0010_0010_0010_0010_0010_0010_0010_0010_0010_0010_0010_0010);

    print(
	0b1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010_1010 ^bitxor 
	0b0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011_0011 == 
	0b1001_1001_1001_1001_1001_1001_1001_1001_1001_1001_1001_1001);

    print(0b1010 ^bitor  0b0011 == 0b1011);
    print(0b1010 ^bitand 0b0011 == 0b0010);
    print(0b1010 ^bitxor 0b0011 == 0b1001);
}

