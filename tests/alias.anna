/*
  Check that user defined method aliases work
*/

type Foo
{

    def bar__String__(String first, String second, String third) (alias(bar))
    {
	print(first, second, third);
    }

    def bar__Int__(Int first, Int second, Int third) (alias(bar))
    {
	print(first + second + third);
    }

}

def main()
{
    var foo = Foo();
    foo.bar(-1,0,1);    
    foo.bar("", "k", "\n");
}
