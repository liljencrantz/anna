
type Foo
{

    def barString(String first, String second, String third) (alias(bar))
    {
	print(first, second, third);
    }
    def barInt(Int first, Int second, Int third) (alias(bar))
    {
	print(first + second + third);
    }
}

def main()
{
    var foo = Foo();
    foo.bar(-1,0,1);    
    foo.bar("", "k", "\n");
}
