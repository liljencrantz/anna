/*
  Check that extending a native template class works
*/

type Derived
(
    extends(MutableList)
)
{
    def __init__()
    {
	MutableList::__init__(this);
    }

}

def main()
{
    vv := Derived();
    vv[3] = 7;
    print(vv, '\n');

    vv2 := Derived«Int»();
    vv2[3] = 7;
    print(vv2, '\n');

}
