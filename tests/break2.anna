def main()
{
    i := 1;
    while(1)
    {
	i++;
	if(i ^mod 2 == 0)
	{
	    continue;
	}
	if( i > 15)
	{
	    break;
	}
	print(i, "\n");
    }

    (0...).each(i)
    {
	if(i>5)
	{
	    break;
	}
	print("infinite ", i, "\n");
    }

    print( (0..1000|5).map(i)
    {
	if(i < 50){
	    continue i*2;
	}
	if(i ^mod 10 == 0)
	{
	    continue i/10;
	}
	if(i > 100)
	{
	    break -1;
	}
	i;
    }, "\n");

    lst := (0..1000|5).map(i){i};
    print( lst.map(i)
    {
	if(i < 50){
	    continue i*2;
	}
	if(i ^mod 10 == 0)
	{
	    continue i/10;
	}
	if(i > 100)
	{
	    break -1;
	}
	i;
    }, "\n");
    
}
