def hasDef1(Int g = 5)
{
    g;
}

def hasDef2(Int a, Int b, Int c, Int d=1, Int e=1, Int f=1, Int g = 1)
{
    a*b*c*d*e*f*g;
}

def hasDef3(Int a=1, Int b=1, Int c, Int d=1, Int e=1, Int f=1, Int g = 1)
{
    a*b*c*d*e*f*g;
}

def main()
{
    print(hasDef1(3), hasDef1());
    print(hasDef2(1,2,3,4), hasDef2(1,2,3,4,5,6,7));
    print(hasDef3(1,2,3,4), hasDef3(b:2, c:3));
    print(hasDef3(1,2,3,4,5,6,7), hasDef3(b:2, c:3, g:4));
}

