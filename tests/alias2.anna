def fun1(Int in) (alias(myFunction)){
    "Int"
}

def fun2(String in) (alias(myFunction)){
    "String"
}

def fun3(Float in) (alias(myFunction)){
    "Float"
}

def fun4(List in) (alias(myFunction)){
    "List"
}

def main(){
    print(myFunction(0))
    print(myFunction(""))
    print(myFunction(0.0))
    print(myFunction([]))
    print(myFunction(? as Int))
    print(myFunction(? as List))
}
