
def main(){
    print((0..10).count, "\n"); // 10
    print((0..10|3).count, "\n"); // 4
    print((0..9|3).count, "\n"); // 3
    print((10..0|-3).count, "\n"); // 4
    print((9..0|-3).count, "\n"); // 3
    print((9..0).count, "\n"); // 9
    print((9..0|3).count, "\n"); // ?
    (9..0|3).each(val){print(val)} // Nothing
}
