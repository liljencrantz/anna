
def myFun( Int a, Int b, def Int fun(Int a, Int b) = Int::__add__Int)
{
    fun(a, b);
}

def myFun2( Int a, Int b, def Int fun(Int a, Int b) = def (Int a, Int b){a+b})
{
    fun(a, b);
}

def main()
{
    print(myFun(1,2), "\n");
    print(myFun(1,2, Int::__sub__Int), "\n");

    print(myFun2(1,2), "\n");
    print(myFun2(1,2, Int::__sub__Int), "\n");
}
