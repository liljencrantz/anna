
class Foo
{
    property Int count getter getCount, setter setCount;
    var Int tralala;
    
    function Int getCount() private
    {
        return this.tralala;
    };

    function setCount(Int val) private
    {
      this.tralala=val;
        return val;
    };

}

class Bar
	extends Foo
{
    
    function ggg()
    {
	return 7;
    }
    
}

var bbb = Bar();
bbb.tralala=7;

print(bbb.count);

